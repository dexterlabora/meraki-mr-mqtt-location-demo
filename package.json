  "private": true,
  "description": "A Cisco Meraki RTLS Monitoring Tool for MQTT",
  "author": "dexterlabora",
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon --watch api --exec nuxt",
